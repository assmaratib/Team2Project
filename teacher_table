CREATE TABLE Teacher(
Teacher_ID INTEGER auto_increment PRIMARY KEY,
Teacher_name VARCHAR(255),
Teacher_last_name VARCHAR(255),
Teacher_email VARCHAR(255),
Teacher_password VARCHAR(50)
);
